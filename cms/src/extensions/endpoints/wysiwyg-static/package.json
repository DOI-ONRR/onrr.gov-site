{
	"name": "wysiwyg-static",
	"description": "Endpoint extension for serving tinymce",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-endpoint"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "endpoint",
		"path": "dist/index.js",
		"source": "src/index.js",
		"host": "^10.10.0"
	},
	"scripts": {
		"build": "directus-extension build; mkdir -p ../../../../extensions/directus-extension-tinymce-static/dist; cp package.json ../../../../extensions/directus-extension-tinymce-static; cp dist/index.js ../../../../extensions/directus-extension-tinymce-static/dist",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"validate": "directus-extension validate"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "16.0.0"
	},
	"dependencies": {
		"serve-static": "^2.2.0"
	}
}
