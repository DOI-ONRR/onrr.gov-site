{
	"name": "onrr-editor",
	"description": "Interface extension based on Editor.js",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-interface"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "interface",
		"path": "dist/index.js",
		"source": "src/index.js",
		"host": "^10.10.0"
	},
	"scripts": {
		"build": "directus-extension build; mkdir -p ../../../../extensions/directus-extension-onrr-editor/dist; cp package.json ../../../../extensions/directus-extension-onrr-editor; cp dist/index.js ../../../../extensions/directus-extension-onrr-editor/dist",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"serve": "vue-cli-service serve"
	},
	"dependencies": {
		"@directus/sdk": "^19.1.0",
		"@editorjs/code": "^2.9.3",
		"@editorjs/editorjs": "^2.30.8",
		"@editorjs/header": "^2.8.8",
		"@editorjs/image": "^2.10.2",
		"@editorjs/link-autocomplete": "^0.1.0",
		"@editorjs/nested-list": "^1.4.3",
		"@editorjs/paragraph": "^2.11.7",
		"@editorjs/quote": "^2.7.6",
		"@editorjs/table": "^2.4.4",
		"codex-notifier": "^1.1.2",
		"core-js": "^3.42.0",
		"cors": "^2.8.5",
		"editorjs-hyperlink": "^1.0.6",
		"editorjs-text-alignment-blocktune": "^1.0.3",
		"editorjs-text-color-plugin": "^2.0.4",
		"vuetify": "^3.8.4"
	},
	"devDependencies": {
		"@babel/core": "^7.27.1",
		"@babel/eslint-parser": "^7.27.1",
		"@directus/extensions-sdk": "13.1.0",
		"@vue/cli-plugin-babel": "~5.0.8",
		"@vue/cli-plugin-eslint": "~5.0.8",
		"@vue/cli-service": "~5.0.8",
		"dotenv": "^16.5.0",
		"eslint": "^9.26.0",
		"eslint-plugin-vue": "^10.1.0",
		"vue": "^3.5.13"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/vue3-essential",
			"eslint:recommended"
		],
		"parserOptions": {
			"parser": "@babel/eslint-parser"
		},
		"rules": {}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not dead",
		"not ie 11"
	]
}
