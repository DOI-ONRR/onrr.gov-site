{
	"name": "onrr-editor-tiny",
	"description": "ONRR Editor (tiny)",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-interface"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "interface",
		"path": "dist/index.js",
		"source": "src/index.js",
		"host": "^10.10.0"
	},
	"scripts": {
		"build": "directus-extension build; mkdir -p ../../../../extensions/directus-extension-onrr-editor-tiny/dist; cp package.json ../../../../extensions/directus-extension-onrr-editor-tiny; cp dist/index.js ../../../../extensions/directus-extension-onrr-editor-tiny/dist",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "^11.0.1",
		"vite": "^5.2.1",
		"vue": "^3.4.21"
	},
	"dependencies": {
		"@directus/sdk": "^15.1.0",
		"@tinymce/tinymce-vue": "^5.1.1",
		"tinymce": "^7.0.0"
	}
}
